{
    "manifest_version": 3,
    "name": "YouTube Shorts Remover",
    "version": "1.0",
    "description": "Removes YouTube Shorts sections from the feed",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }